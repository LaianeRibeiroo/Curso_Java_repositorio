package Testes_executados_em_aulas;

import java.util.Scanner;

public class Aula_45 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		int x = sc.nextInt();
		int soma;
		while (x != 0) {
			soma += x;
			x = sc.nextInt();
		}
       printf ("soma =  %d",soma);
		sc.close();
	}

}
